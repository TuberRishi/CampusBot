# The API key for Google Gemini Pro, used as the primary LLM
GEMINI_API_KEY="YOUR_GEMINI_API_KEY_HERE"

# The API key for LangSmith, used for tracing and debugging the LLM application
LANGCHAIN_API_KEY="YOUR_LANGCHAIN_API_KEY_HERE"

# Set this to "true" to enable LangSmith tracing
LANGCHAIN_TRACING_V2="true"

# The name of the project in LangSmith
LANGCHAIN_PROJECT="YOUR_LANGSMITH_PROJECT_NAME_HERE"

# (Optional) The API key for a translation service, if used
# TRANSLATION_API_KEY="YOUR_TRANSLATION_API_KEY_HERE"
